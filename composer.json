{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "alexandr-mironov/php-smpp": "1.3.0.9",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "bmatovu/laravel-mtn-momo": "^4.2",
        "bugsnag/bugsnag-laravel": "^2.0",
        "doctrine/dbal": "^3.0",
        "engageso/engage-php": "^1.1",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "^2.7",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^1.5",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.0",
        "lcobucci/jwt": "^4.0",
        "maatwebsite/excel": "^3.1",
        "pragmarx/google2fa-laravel": "^2.0",
        "staudenmeir/eloquent-json-relations": "*",
        "stripe/stripe-php": "^16.1",
        "toin0u/geocoder-laravel": "^4.4",
        "torann/geoip": "^3.0",
        "twilio/sdk": "^8.0",
        "tymon/jwt-auth": "^2.0",
        "unicodeveloper/laravel-paystack": "^1.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "kitloong/laravel-migrations-generator": "^7.2",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan cache:clear"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan cache:clear"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}